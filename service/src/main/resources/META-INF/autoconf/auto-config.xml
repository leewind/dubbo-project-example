<?xml version="1.0" encoding="UTF-8"?>

<config description="milkt.config">
  <group name="milkt.onelab.config.setting">
    <property name="milkt.onelab.organization.db.url"  description="Demo数据库地址" required="true"/>
    <property name="milkt.onelab.organization.db.username"  description="Demo数据库用户名" required="true"/>
    <property name="milkt.onelab.organization.db.password"  description="Demo数据库密码" required="true"/>

    <property name="milkt.onelab.organization.log.home"  description="Demo日志文件根目录" required="true"/>

    <property name="milkt.dubbo.registry.url"  description="Dubbo注册中心地址" required="true"/>
    <property name="milkt.onelab.organization.dubbo.export.timeout" defaultValue="3000" description="Dubbo服务超时时间" required="true"/>
    <property name="milkt.onelab.organization.dubbo.reference.version"  description="Dubbo服务引用版本号" required="true"/>
  </group>

  <script>
    <generate template="config.properties.vm" destfile="config.properties" charset="UTF-8" />
  </script>
</config>
